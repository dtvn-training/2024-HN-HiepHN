public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{

    Object[] r = getRow();
 
    if (r == null) {
      setOutputDone();
      return false;
    }
 
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 
    String currentPrice = get(Fields.In, "current_price").getString(r);
    String originalPrice =  get(Fields.In, "original_price").getString(r);
   
    if (originalPrice.equals("-1")){
    get(Fields.Out, "original_price").setValue(outputRow, currentPrice);
 }
    putRow(data.outputRowMeta, outputRow);
    return true;
}]]></class_source>
