    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.regex.Matcher; 
import java.util.regex.Pattern;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{

    Object[] r = getRow();
 
    if (r == null) {
      setOutputDone();
      return false;
    }
 
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
 
    String review = get(Fields.In, "review").getString(r);

    String [] reviewArr = review.split("],");

    String len = String.valueOf(reviewArr.length);
    
    String transformReview ="";

    for (int i=0;i<reviewArr.length;i++){
        String s=reviewArr[i];
        String originalString=s.substring(1,s.length());
    	int commaIndex = originalString.indexOf(',');

        int brackIndex = originalString.indexOf(", [");

    	if (commaIndex != -1) {
         	String ratingSplit = "<span><img alt=\"star-icon\" src=\"https://salt.tikicdn.com/ts/upload/e3/f0/86/efd76e1d41c00ad8ebb7287c66b559fd.png\" width=\"20\" height=\"20\"></span>";
         	String ratingString = originalString.substring(0, commaIndex);
            int rating = (ratingString.split(ratingSplit, -1).length - 1)-5;

            String imgString;

            if(i!= reviewArr.length-1)
            imgString = originalString.substring(brackIndex+3,originalString.length()-1);
            else
 			imgString = originalString.substring(brackIndex+3,originalString.length()-3);
            String contentString = originalString.substring(commaIndex+1,brackIndex);

            String [] reviewImgArr = imgString.split(", "); 
            String transformImgString = "";
            for (int j=0;j<reviewImgArr.length;j++){

                Pattern pattern = Pattern.compile("url\\(\"(.*?)\"\\)"); 
                Matcher matcher = pattern.matcher(reviewImgArr[j]);

                if(matcher.find()){
                    transformImgString=transformImgString+matcher.group(1);
                    if(j!=reviewImgArr.length-1) transformImgString=transformImgString+";";
                }


			}


         	String newString = String.valueOf(rating)+"++"+contentString+"++"+transformImgString;


            transformReview=transformReview + newString;
            
            if (i!=reviewArr.length-1) transformReview = transformReview +"|";

    }
}
   
    get(Fields.Out, "review").setValue(outputRow, transformReview);
    // putRow will send the row on to the default output hop.
    //
    putRow(data.outputRowMeta, outputRow);
    return true;
}]]></class_source>
        </definition>
